{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "no-underscore-dangle": [2, { "allow": ["_id"] }],
        "no-console": [
          "warn",
          { "allow": ["error"] }
        ]
    }
}
